{
  "name": "maeple",
  "private": true,
  "version": "0.97.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "node scripts/health-check.cjs && npm run dev",
    "health": "node scripts/health-check.cjs",
    "build": "tsc && vite build",
    "build:production": "vite build",
    "preview": "vite preview",
    "analyze": "node scripts/analyze-bundle.cjs",
    "build:analyze": "npm run build && npm run analyze",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ai": "node scripts/test-ai-providers.cjs",
    "test:all": "npm run build && npm run analyze && npm run test:ai",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "check-all": "npm run lint && npm run typecheck && npm run test:run",
    "fix-all": "npm run lint:fix && npm run format && npm run typecheck"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capgo/capacitor-health": "^8.0.1",
    "@google/genai": "^0.14.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "idb": "^8.0.0",
    "jsonwebtoken": "^9.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.10.1",
    "react-virtuoso": "^4.17.0",
    "recharts": "^2.12.0",
    "tailwind-merge": "^2.2.0",
    "uuid": "^9.0.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  }
}
